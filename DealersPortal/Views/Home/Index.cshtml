@{
    ViewBag.Title = "Home Page";
}
<!-- Header Carousel -->
</div>
<header id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <div class="fill" style="background-image:url('../images/tow-truck-950x430.jpg');"></div>
            <div class="carousel-caption">
                <h2>Vehicle Towing Assistance</h2>
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image:url('../images/plumber.jpg');"></div>
            <div class="carousel-caption">
                <h2>Plumbing Assistance</h2>
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image:url('../images/locksmith-1.jpg');"></div>
            <div class="carousel-caption">
                <h2>Locksmith Assistance</h2>
            </div>
        </div>
    </div>
    <!-- Controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="icon-next"></span>
    </a>
</header>
